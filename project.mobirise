            },
            "textarea::-moz-placeholder": {
              "color": "@labelsColor"
            },
            ".soc-item": {
              "display": "inline-block",
              "margin": "0 .1rem .2rem .1rem",
              "padding": "10px 11px 0 11px",
              "border": "1px solid @iconsColor",
              "border-radius": "50%",
              "&:hover": {
